<?xml version='1.0' encoding='utf-8'?>
<root main_tree_to_execute='HumanWarehouseDutiesTree'>
    <include path='BTRegularNav.xml'/>
    <BehaviorTree ID='HumanWarehouseDutiesTree'>
        <Fallback name='MainTreeFallback'>
            <Sequence name='RobotBlockingSequence'>
                <IsRobotVisible agent_id='{id}' distance='0.5'/>
                <IsRobotBlocking agent_id='{id}' distance='0.5'/>
                <Inverter name='WaitUntilRobotPasses'>
                    <IsRobotBlocking agent_id='{id}' distance='0.5'/>
                </Inverter>
            </Sequence>
            <Sequence name='RegNav'>
                <SetBlackboard output_key='agentid' value='{id}'/>
                <SetBlackboard output_key='timestep' value='{dt}'/>
                <SubTree ID='RegularNavTree' id='agentid' dt='timestep'/>
            </Sequence>
        </Fallback>
    </BehaviorTree>
</root>