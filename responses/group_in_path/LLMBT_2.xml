<root main_tree_to_execute='Human3BehaviorTree'>
    <BehaviorTree ID='Human3BehaviorTree'>
        <Fallback name='CheckAndGiveWay'>
            <Sequence name='GiveWaySequence'>
                <RobotSays agent_id='{id}' message='4'/>
                <IsRobotNearby agent_id='{id}' distance='1.0'/>
                <GiveWaytoRobot agent_id='{id}' time_step='{dt}'/>
            </Sequence>
            <MakeGesture agent_id='{id}' message='0'/>
        </Fallback>
    </BehaviorTree>
</root>